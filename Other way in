
void setup()
{ 
waitForDrivers ();

main ()
}





void waitForDrivers()
{
while (!(keyboard_leds & 2))

    {    key(KEY_CAPS_LOCK, 0);   }  

if (keyboard_leds & 2)

    {   key(KEY_CAPS_LOCK, 0);  }

}





void main()
 {

DigiKeyboard.sendKeyStroke(KEY_F10 , MOD_SHIFT_LEFT);
  DigiKeyboard.delay(500);  
  DigiKeyboard.sendKeyStroke(KEY_AKEY_ARROW_DOWN);
